{
  "name": "responsive-react-restaurant-website",
  "homepage": "https://savanikrishna95.github.io/react-responsive-demo",
  "private": true,
  "version": "0.1.2",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "generate-meta-tag": "node ./node_modules/react-cache-buster/dist/generate-meta-tag.js",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist && TIMESTAMP=$(date '+%Y%m%d%H%M%S') webpack --mode production --env.timestamp=\"$TIMESTAMP\"",
    "build": "npm run generate-meta-tag && TIMESTAMP=$(date '+%Y%m%d%H%M%S') vite build --mode=production --env.timestamp=$TIMESTAMP",
    "preview": "vite preview"
  },

  
  "dependencies": {
    "gh-pages": "^6.1.1",
    "react": "^18.2.0",
    "react-cache-buster": "^0.1.8",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.43.5",
    "react-icons": "^4.8.0",
    "react-is": "^18.2.0",
    "react-router-dom": "^6.9.0",
    "react-tilt": "^0.1.4",
    "styled-components": "^5.3.8"
  },
  "devDependencies": {
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "@vitejs/plugin-react": "^3.1.0",
    "react-app-rewired": "^2.2.1",
    "vite": "^4.1.0"
  }
}
